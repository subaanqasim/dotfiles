{
  "editor.linkedEditing": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.wordWrap": "on",
  "cSpell.language": "en-GB",
  "editor.detectIndentation": false,
  "workbench.startupEditor": "none",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "workbench.colorCustomizations": {},
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "Material Theme Darker High Contrast",
  "workbench.preferredHighContrastColorTheme": "Material Theme Darker High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "Material Theme Lighter High Contrast",
  "workbench.preferredLightColorTheme": "Material Theme Lighter High Contrast",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "0", "p"]
    },
    {
      "before": ["<leader>", "P"],
      "after": ["\"", "0", "P"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<C-j>"],
      "after": ["g", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["g", "k"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    }, // moveLineDown
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    }, // moveLineUp
    {
      "before": ["H"],
      "after": ["^"]
    }, // move cursor to beginning of line
    {
      "before": ["L"],
      "after": ["$"]
    }, // move cursor to end of line
    // {
    //   "before": ["p"],
    //   "commands": ["editor.action.clipboardPasteAction"],
    //   "after": ["<Esc>"]
    // }
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    }, // move down half page and center cursor
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    }, // move up half page and center cursor
    {
      "before": ["n"],
      "after": ["n", "z", "z"]
    }, // move to next search result and center cursor
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    } // move to previous search result and center cursor
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "0", "p"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    }, // move cursor to beginning of line
    {
      "before": ["L"],
      "after": ["$"]
    }, // move cursor to end of line
    {
      "before": ["<C-j>"],
      "after": ["g", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["g", "k"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-c>": false,
    "<C-v>": false
  },
  "vim.smartRelativeLine": true,
  "editor.lineNumbers": "relative",
  "thunder-client.codeSnippetLanguage": "cs-httpclient",
  "editor.inlineSuggest.enabled": true,
  "importCost.smallPackageSize": 35,
  "importCost.mediumPackageSize": 75,
  "files.eol": "\n",
  "importCost.timeout": 10000,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "editor.fontFamily": "'FiraCode Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "notebook.lineNumbers": "on",
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "window.autoDetectColorScheme": true,
  "editor.fontLigatures": true,
  "editor.stickyScroll.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "projectManager.groupList": true,
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "totalTypeScript.hiddenTips": [
    "passing-generics-to-types",
    "non-null-expression",
    "typeof",
    "partial-utility-type",
    "omit-utility-type",
    "pick-utility-type",
    "generic-slots-in-functions",
    "type-alias-with-generics",
    "type-alias-with-multiple-generics",
    "keyof",
    "tuple-type",
    "as-const",
    "in-operator-narrowing",
    "as-const-on-object",
    "returntype-utility-type",
    "required-utility-type",
    "record-utility-type",
    "awaited-utility-type",
    "mapped-type",
    "conditional-type",
    "nested-conditional-type",
    "never-keyword",
    "nonnullable-utility-type",
    "infer"
  ],
  "gitlens.rebaseEditor.ordering": "asc",
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.minimap.enabled": false
}
